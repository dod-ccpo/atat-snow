<?xml version="1.0" encoding="UTF-8"?><database>
    <element label="DAPPS:Current Environment Instance" max_length="40" name="x_g_dis_atat_current_environment_instance" type="collection">
        <element label="Additional Information" max_length="40" name="additional_information" type="string"/>
        <element label="Classification Level" max_length="32" name="classification_level" reference="x_g_dis_atat_classification_level" type="reference"/>
        <element label="Current Payment Arrangement" max_length="40" name="current_payment_arrangement" type="string"/>
        <element choice="1" label="Current Usage Description" max_length="40" name="current_usage_description" type="choice">
            <choice>
                <element inactive_on_update="false" label="Evenly distributed or constant usage" sequence="1" value="EVEN_OR_CONSTANT"/>
                <element inactive_on_update="false" label="Irregular usage or spike traffic" sequence="2" value="IRREGULAR_OR_SPIKE"/>
            </choice>
        </element>
        <element label="Data Egress Monthly Amount" max_length="40" name="data_egress_monthly_amount" type="integer"/>
        <element choice="1" label="Data Egress Monthly Unit" max_length="40" name="data_egress_monthly_unit" type="choice">
            <choice>
                <element inactive_on_update="false" label="GB" sequence="1" value="GB"/>
                <element inactive_on_update="false" label="TB" sequence="2" value="TB"/>
                <element inactive_on_update="false" label="PB" sequence="3" value="PB"/>
            </choice>
        </element>
        <element label="Deployed Regions" max_length="4000" name="deployed_regions" reference="x_g_dis_atat_dapps_region" type="glide_list"/>
        <element label="Licensing" max_length="40" name="licensing" type="string"/>
        <element label="Memory Amount" max_length="40" name="memory_amount" type="integer"/>
        <element choice="1" default="GB" label="Memory Unit" max_length="40" name="memory_unit" type="choice">
            <choice>
                <element inactive_on_update="false" label="GB" sequence="1" value="GB"/>
                <element inactive_on_update="false" label="TB" sequence="2" value="TB"/>
                <element inactive_on_update="false" label="PB" sequence="3" value="PB"/>
            </choice>
        </element>
        <element label="Number of Similar Instances" max_length="40" name="number_of_similar_instances" type="integer"/>
        <element label="Number of  vCPUs" max_length="40" name="number_of_vcpus" type="integer"/>
        <element label="Operating System" max_length="40" name="operating_system" type="string"/>
        <element label="Pricing Period Expiration Date" max_length="40" name="pricing_period_expiration_date" type="glide_date"/>
        <element label="Processor Speed" max_length="40" name="processor_speed" type="string"/>
        <element label="Storage Amount" max_length="40" name="storage_amount" type="integer"/>
        <element choice="1" label="Storage Unit" max_length="40" name="storage_unit" type="choice">
            <choice>
                <element inactive_on_update="false" label="GB" sequence="1" value="GB"/>
                <element inactive_on_update="false" label="TB" sequence="2" value="TB"/>
                <element inactive_on_update="false" label="PB" sequence="3" value="PB"/>
            </choice>
        </element>
        <element label="Surge Usage: Event-based spike in traffic" max_length="40" name="surge_usage_event_based_spike_in_traffic" type="boolean"/>
        <element label="Surge Usage: High usage during certain periods" max_length="40" name="surge_usage_high_usage_during_certain_periods" type="boolean"/>
        <element label="Users per Region" max_length="4000" name="users_per_region" type="simple_name_values"/>
        <index name="index">
            <element name="classification_level"/>
        </index>
    </element>
</database>
