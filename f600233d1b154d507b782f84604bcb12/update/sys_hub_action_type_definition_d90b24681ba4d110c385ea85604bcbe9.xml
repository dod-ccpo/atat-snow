        <sys_updated_on>2022-07-04 05:21:24</sys_updated_on>
        <value>(function execute(inputs, outputs) {
  
  outputs.docusign_recipients = []
  while (inputs.contacts.next()) {
    var contact = inputs.contacts;
    var order, roleName;
    switch (contact.type.toString()) {
      case "MISSION_OWNER":
        order = 1
        roleName = "Mission Owner"
        break
      case "JWCC_CONTRACTING_OFFICER":
        order = 2
        roleName = "JWCC Contracting Officer"
        break
      case "FINANCIAL_POC":
        order = 3
        roleName = "Financial POC"
        break
      default:
        order = undefined
        roleName = undefined
        continue
    }
    
    outputs.docusign_recipients.push({
      formal_name: contact.formal_name.toString(),
      email: contact.email.toString(),
      role_name: roleName,
      recipient_order: order.toPrecision(1),
      contact_type: contact.type.getDisplayValue()
    })  
  }
  
})(inputs, outputs);
</value>
