    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_g_dis_atat.convertToBoolean</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Convert "Yes/No/Unselected" choices to a boolean or make null</description>
        <name>convertToBoolean</name>
        <script><![CDATA[function convertToBoolean(string) {
	var choice = string.toLowerCase();
	if (choice === "yes" || choice === "true") {
		return true;
	}
	if (choice === "no" || choice === "false") {
		return false;
	}
	return null;
}]]></script>
